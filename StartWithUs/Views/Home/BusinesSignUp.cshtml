@model StartWithUs.Models.BusinessSignUpModel
@{
    ViewBag.Title = "BusinesSignUp";
    //Layout = "~/Views/_LayoutPage1.cshtml";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Business SignUp</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="~/css/._main.css" rel="stylesheet" />
    <link href="~/css/main.css" rel="stylesheet" />
    <link href="~/css/._util.css" rel="stylesheet" />
    <link href="~/css/util.css" rel="stylesheet" />
    <script src="~/js/._main.js"></script>
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="css/util.css">
    <link rel="stylesheet" type="text/css" href="css/main.css">

</head>
<body>

    <div class="limiter">
        <div class="container-login100" style="background-image: url('../../images/bg-01.jpg');">
            <div class="wrap-login100 p-l-110 p-r-110 p-t-62 p-b-33">
                @using (Html.BeginForm("BusinesSignUp", "Home", FormMethod.Post))
                {
                    if (ViewBag.Status != null)
                    {
                        if (ViewBag.Status == 1)
                        {
                            <span class="alert alert-success">Registration Successful</span>
                            <br /><br />
                        }
                        else if (ViewBag.Status == 0)
                        {
                            <span class="alert alert-danger">Error while registering</span>
                        }
                    }
                    @*<form class="login100-form validate-form flex-sb flex-w">*@
                    <span class="login100-form-title p-b-53">
                        Business Sign Up
                    </span>
                    <div class="p-t-31 p-b-9">
                        <span class="txt1">
                            Full Name <b style="color:red">*</b>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="First Name is required">
                        @Html.TextBoxFor(m => m.Name, new { @class = "input100", @type = "text", @name = "FirstName" })
                        @*<input class="input100" type="text" name="FirstName">*@
                        <span class="focus-input100"></span>
                    </div>

                    <div class="p-t-13 p-b-9">
                        <span class="txt1">
                            Enterpreneur ID <b style="color:red">*</b>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Enterpreneur ID is required">
                        @Html.TextBoxFor(m => m.EnterpreneurId, new { @class = "input100", @type = "text", @name = "EnterpreneurId" })
                        <span class="focus-input100"></span>
                    </div>
                    <div class="p-t-13 p-b-9">
                        <span class="txt1">
                            PAN Number <b style="color:red">*</b>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="PAN Number is required">
                        @Html.TextBoxFor(m => m.PAN, new { @class = "input100", @type = "text", @name = "PAN" })
                        @*<input class="input100" type="date" name="DOB">*@
                        <span class="focus-input100"></span>
                    </div>
                    <div class="p-t-13 p-b-9">
                        <span class="txt1">
                            Email Id <b style="color:red">*</b>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Email is required">
                        @Html.TextBoxFor(m => m.Email, new { @class = "input100", @type = "email", @name = "Email" })
                        @*<input class="input100" type="text" name="Email">*@
                        <span class="focus-input100"></span>
                    </div>
                    <div class="p-t-13 p-b-9">
                        <span class="txt1">
                            Mobile Number <b style="color:red">*</b>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Mobile Number is required">
                        @Html.TextBoxFor(m => m.MobileNo, new { @class = "input100", @type = "text", @name = "mobile" })
                        @*<input class="input100" type="text" name="MobNum">*@
                        <span class="focus-input100"></span>
                    </div>
                    <div class="p-t-13 p-b-9">
                        <span class="txt1">
                            Date of Birth <b style="color:red">*</b>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Date of Birth is required">
                        @Html.TextBoxFor(m => m.DOB, new { @class = "input100", @type = "date", @name = "dob" })
                        @*<input class="input100" type="date" name="DOB" >*@
                        <span class="focus-input100"></span>
                    </div>
                    <div class="p-t-13 p-b-9">
                        <span class="txt1">
                            Address <b style="color:red">*</b>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Address is required">
                        @Html.TextAreaFor(m => m.Address, new { @class = "input100", @type = "text", @name = "address" })
                        @*<input class="input100" type="date" name="DOB" >*@
                        <span class="focus-input100"></span>
                    </div>
                    <div class="p-t-13 p-b-9">
                        <span class="txt1">
                            Password <b style="color:red">*</b>
                        </span>
                    </div>
                    <div class="wrap-input100 validate-input" data-validate="Password is required">
                        <i class="fa-fast-backward fa-key"></i>
                        @Html.PasswordFor(m => m.Password, new { @class = "input100", @type = "password", @name = "pass" })
                        @*<input class="input100" type="password" name="pass">*@
                        <span class="focus-input100"></span>
                    </div>

                    <div class="container-login100-form-btn m-t-17">
                        <button class="login100-form-btn" onclick="~/Home/Index">
                            Sign Up
                        </button>
                    </div>
                }
            </div>
    </div>


    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="vendor/bootstrap/js/popper.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="vendor/daterangepicker/moment.min.js"></script>
    <script src="vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="js/main.js"></script>

</body>
</html>





